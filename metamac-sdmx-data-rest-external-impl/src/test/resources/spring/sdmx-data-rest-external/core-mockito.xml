<?xml version="1.0" encoding="UTF-8"?>
<beans  xmlns="http://www.springframework.org/schema/beans"
        xmlns:p="http://www.springframework.org/schema/p"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"    
        xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

    <bean id="datasetRepositoriesServiceFacade" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="com.arte.statistic.dataset.repository.service.DatasetRepositoriesServiceFacade" />
    </bean>
    
    <bean id="datasetService" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="org.siemac.metamac.statistical.resources.core.dataset.serviceapi.DatasetService" />
    </bean>
    
    <bean id="apisLocator" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="org.siemac.metamac.sdmx.data.rest.external.v2_1.service.ApisLocator" />
    </bean>
    
</beans>