<?xml version="1.0" encoding="UTF-8"?>
<beans  xmlns="http://www.springframework.org/schema/beans"
        xmlns:p="http://www.springframework.org/schema/p"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"    
        xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

    <bean id="organisationsService" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="com.arte.statistic.sdmx.srm.core.organisation.serviceapi.OrganisationsService" />
    </bean>
    
    <bean id="conceptsService" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="com.arte.statistic.sdmx.srm.core.concept.serviceapi.ConceptsService" />
    </bean>
    
    <bean id="codesService" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="com.arte.statistic.sdmx.srm.core.code.serviceapi.CodesService" />
    </bean>
    
    <bean id="categoriesService" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="com.arte.statistic.sdmx.srm.core.category.serviceapi.CategoriesService" />
    </bean>
    
    <bean id="dataStructureDefinitionService" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="com.arte.statistic.sdmx.srm.core.structure.serviceapi.DataStructureDefinitionService" />
    </bean>
    
    <bean id="codeRepository" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="com.arte.statistic.sdmx.srm.core.code.domain.CodeRepository" />
    </bean>    
    
    <bean id="conceptRepository" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="com.arte.statistic.sdmx.srm.core.concept.domain.ConceptRepository" />
    </bean>    
    
    <bean id="itemSchemeVersionRepository" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="com.arte.statistic.sdmx.srm.core.base.domain.ItemSchemeVersionRepository" />
    </bean> 
    
    <bean id="organisationSchemeVersionRepository" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="com.arte.statistic.sdmx.srm.core.organisation.domain.OrganisationSchemeVersionRepository" />
    </bean> 
    
